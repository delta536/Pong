    public Pong(int p1, int p2) { 
    }
    //key pressed and released
     public void keyPressed () {

        if (getKeyCode() == KeyEvent.Down)
            key_up = true;
        else    
        { if (getKeyCode() == KeyEvent.Down)
            key_down = true;
    }
}
    public void keyReleased () {
        if (getKeyCode() == KeyEvent.Down)
           key_up = false;
        else if (getKeyCode() == KeyEvent.Down)
           key_down = false;
       }
    
}
